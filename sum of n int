#include<stdio.h>
#include<conio.h>
int sum(int n,int a[])
{
  int sum=0,i;
  for(i=1;i<=n;i++)
   sum=sum+a[i];
  return sum;
}  
void main()
{
  int a[],n,sum,i;
  printf("enter number of elements\n");
  scanf("%d",&n);
  printf("enter array elements\n");
  for(i=1;i<=n;i++)
  scanf("%d",&a[i]);
  sum=sum(a[],n);
  printf("sum is %d\n",sum);
  getch();
}  
