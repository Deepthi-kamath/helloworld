#include<stdio.h>
#include<conio.h>
#include<math.h>
float areaoftriangle(int b,int h)
{
    float area;
    area=(0.5*b*h);
    return area;
}
int main()
{
    int p,q;
    float r;
    printf("enter base and height");
    scanf("%d%d",&p,&q);
    r=(0.5*p*q);
    printf("area of triangle is given by %f",r);
    return 0;
}
